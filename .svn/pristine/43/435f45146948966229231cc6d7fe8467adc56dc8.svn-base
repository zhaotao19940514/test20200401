package cn.com.taiji.css.entity.dict;

public enum ResourceType
{
	B_KHFW("开户服务", "xtgl.png","fa fa-cog") {},
	C_SHFW("售后服务", "xtgl.png","fa fa-cog") {},
	D_FPFW("发票服务", "xtgl.png","fa fa-cog") {},
	E_XZGL("行政管理", "xtgl.png","fa fa-cog") {},
	Z_XTGL("系统管理", "xtgl.png","fa fa-cog") {},
	;

	private final String value;
	private final String logoPic;
	private final String logoClass;

	private ResourceType(String value, String logoPic, String logoClass)
	{
		this.value = value;
		this.logoPic = logoPic;
		this.logoClass = logoClass;
	}

	public String getLogoClass()
	{
		return logoClass;
	}

	public String getValue()
	{
		return value;
	}

	public String getLogoPic()
	{
		return logoPic;
	}
}